{
  "name": "@solid-soda/tslint-config",
  "description": "TSLint config for Solid Soda team",
  "version": "2.5.1",
  "author": "Vladimir Golotin (https://github.com/Uncleseneca)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solid-soda/tslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/solid-soda/tslint-config/issues"
  },
  "scripts": {
    "release": "standard-version"
  },
  "keywords": [
    "tslint",
    "typescript",
    "tslintconfig",
    "config",
    "soda",
    "solid-soda",
    "styleguide"
  ],
  "dependencies": {
    "tslint-eslint-rules": "^5.4.0",
    "tslint-origin-ordered-imports-rule": "jens-duttke/tslint-origin-ordered-imports-rule"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.3.2",
    "@commitlint/config-conventional": "^7.3.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "homepage": "https://github.com/solid-soda/tslint-config/#readme",
  "license": "MIT",
  "main": "tslint.json",
  "peerDependencies": {
    "tslint": "^5.8.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
